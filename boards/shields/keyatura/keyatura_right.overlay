#include "keyatura.dtsi"
#include "cirque.dtsi"

&kscan0 {
    col-gpios
        = <&gpio0 9 GPIO_ACTIVE_HIGH>
        , <&gpio0 10 GPIO_ACTIVE_HIGH>
        , <&gpio1 11 GPIO_ACTIVE_HIGH>
        , <&gpio1 13 GPIO_ACTIVE_HIGH>
        , <&gpio1 15 GPIO_ACTIVE_HIGH>
        ;
};

&default_transform {
	col-offset = <5>;
};

&spi1 {
    status = "okay";
};

&glidepoint {
    status = "okay";
};

&glidepoint_split {
    device = <&glidepoint>;
};